# Environment variables for `docker-compose.yml`.
#
# Docker Compose automatically loads `.env` from the repo root for variable
# substitution. Copy this file to `.env` and adjust as needed.
#
# IMPORTANT: Do not commit real secrets.

# --- InkPass ---
INKPASS_ENCRYPTION_KEY=dev-encryption-key-32-chars-long
INKPASS_SECRET_KEY=dev-secret-key-change-in-production
INKPASS_JWT_SECRET_KEY=dev-jwt-secret-key-change-in-production

# --- Tentackl ---
TENTACKL_API_KEY=sk_tentackl_dev

# --- Mimic ---
MIMIC_SERVICE_API_KEY=sk_mimic_service_dev
MIMIC_ENCRYPTION_KEY=dev-encryption-key-32-chars-long
MIMIC_SECRET_KEY=dev-secret-key-change-in-production

# --- Webhook gateway (optional) ---
STRIPE_SECRET_KEY=
STRIPE_WEBHOOK_SECRET=
